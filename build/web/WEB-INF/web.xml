<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RegisterUserValidate</servlet-name>
        <servlet-class>RegisterUserValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loginvalidate</servlet-name>
        <servlet-class>Loginvalidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetParameters</servlet-name>
        <servlet-class>GetParameters</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>Controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterUserValidate</servlet-name>
        <url-pattern>/RegisterUserValidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loginvalidate</servlet-name>
        <url-pattern>/Loginvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetParameters</servlet-name>
        <url-pattern>/GetParameters</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/Controller</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
